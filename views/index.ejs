  <div class="container">
    <h2>URL Shrinker</h2>
    <form action="/shortUrls" method="POST" class="my-4 form-inline">
      <label for="fullUrl" class="sr-only">Url</label>
      <input required placeholder="Enter Url" type="url" name="full" id="full" class="form-control col mr-2">
      <button class="btn btn-success" type="submit">Shrink</button>
    </form>
    </div>
          
    
    <div class="links">
        <div class="link-box">
            <div class="full">
                <h2 style="font-size: 16px; font-weight: bold;">Full Link</h2>
            </div>
            <div class="short">
                <h2 style="font-size: 16px; font-weight: bold;">Short Link</h2>
            </div>
            <div class="click">
                <h2 style="font-size: 16px; font-weight: bold;">Clicks</h2>
            </div>
        </div>
    </div>
    
    
      <%   shorturls.forEach(url => { %>
        <div class="links">
            <div class="link-box">
               <div class="full">
                  <a href="<%= url.full %>"><%= url.full %></a>
               </div>
               

               <!-- Short Link Section -->
<div class="short">
    <h2 style="font-size: 16px; font-weight: bold;">Short Link</h2>
    <div style="display: flex; align-items: center;">
        <!-- Short Link -->
        <a target="_blank" href="<%= url.short %>"><%= url.short %></a>
        <!-- Clipboard Icon -->
        <button onclick="copyToClipboard('<%= url.short %>')" style="background: none; border: none; cursor: pointer; margin-left: 5px;">
            <i class="far fa-clipboard"></i> <!-- Clipboard Icon -->
        </button>
    </div>
</div>       
               <div class="clicks">
                  <span><%= url.clicks %></span>
               </div>
               <div class="delete">
                <a href="/delete/<%= url._id %>">
                    <span class="material-symbols-outlined">
                        auto_delete
                        </span>
                </a>
              </div>
            </div>
        </div>
      <%});  %>
    </div>

